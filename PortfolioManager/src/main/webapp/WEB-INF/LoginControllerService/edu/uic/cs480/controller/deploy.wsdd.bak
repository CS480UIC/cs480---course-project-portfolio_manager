<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from LoginControllerService WSDL service -->

  <service name="LoginController" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://controller.cs480.uic.edu"/>
      <parameter name="wsdlServiceElement" value="LoginControllerService"/>
      <parameter name="schemaQualified" value="http://controller.cs480.uic.edu,http://model.cs480.uic.edu"/>
      <parameter name="wsdlServicePort" value="LoginController"/>
      <parameter name="className" value="edu.uic.cs480.controller.LoginControllerSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="LoginController"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getUser" qname="operNS:getUser" xmlns:operNS="http://controller.cs480.uic.edu" returnQName="retNS:getUserReturn" xmlns:retNS="http://controller.cs480.uic.edu" returnType="rtns:User" xmlns:rtns="http://model.cs480.uic.edu" soapAction="" >
        <parameter qname="pns:userName" xmlns:pns="http://controller.cs480.uic.edu" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://controller.cs480.uic.edu" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="testService" qname="operNS:testService" xmlns:operNS="http://controller.cs480.uic.edu" returnQName="retNS:testServiceReturn" xmlns:retNS="http://controller.cs480.uic.edu" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:userName" xmlns:pns="http://controller.cs480.uic.edu" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getUser testService"/>

      <typeMapping
        xmlns:ns="http://model.cs480.uic.edu"
        qname="ns:User"
        type="java:edu.uic.cs480.model.User"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
